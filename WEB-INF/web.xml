<!DOCTYPE web-app PUBLIC
  '-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN'
  'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app>

	<servlet>
		<servlet-name>HomePage</servlet-name>
		<servlet-class>HomePage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HomePage</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>hello</servlet-name>
		<servlet-class>HelloTomcat</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>getname</servlet-name>
		<servlet-class>GetName</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getname</servlet-name>
		<url-pattern>/getname</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>query</servlet-name>
		<servlet-class>net.impjq.database.QueryDatabase</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>query</servlet-name>
		<url-pattern>/query</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>AddAccount</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TwitterUpdateStatus</servlet-name>
		<servlet-class>net.impjq.twitter.UpdateStatusImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TwitterUpdateStatus</servlet-name>
		<url-pattern>/s1/UpdateStatus</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>FacebookUpdateStatus</servlet-name>
		<servlet-class>net.impjq.facebook.UpdateStatusImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FacebookUpdateStatus</servlet-name>
		<url-pattern>/s2/UpdateStatus</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>OldWebRegister</servlet-name>
		<servlet-class>net.impjq.account.web.OldRegister</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OldWebRegister</servlet-name>
		<url-pattern>/account/web/OldRegister</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>WebRegister</servlet-name>
		<servlet-class>net.impjq.account.web.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebRegister</servlet-name>
		<url-pattern>/account/web/Register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>WebAccountUpdate</servlet-name>
		<servlet-class>net.impjq.account.web.Update</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebAccountUpdate</servlet-name>
		<url-pattern>/account/web/Update</url-pattern>
	</servlet-mapping>

</web-app>